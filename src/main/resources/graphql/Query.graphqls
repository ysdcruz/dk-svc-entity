# The query root of DevKinetics' Entity Service GraphQL interface
type Query {

    ## APP

    # Return an app
    app(

        # ID of the object
        pk_app_id: ID,

        # Get app by merchant ID
        merchant_id: String

    ): App

    # Return all apps
    allApps: [App]

    ## GROUP

    # Return a group
    group(

        # ID of the object.
        pk_group_id: ID!

    ): Group

    # Return all groups
    allGroups: [Group]

}